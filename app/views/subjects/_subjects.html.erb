
<% subjects&.each do |subject| %>

 <% params = {
    id: subject.id,
    title: subject.title,
    body: subject.body,
    user_full_name: subject.user.full_name,
    replies_count: subject.replies.count,
    view_count: subject.impressionist_count } %>



  <%= react_component "Subject", {params: params} %>

<% end %>
